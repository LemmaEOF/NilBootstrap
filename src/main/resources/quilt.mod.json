{
	"schema_version": 1,
	"quilt_loader": {
		"group": "gay.lemmaeof",
		"id": "nilbootstrap",
		"version": "${version}",
		"metadata": {
			"name": "NilBootstrap",
			"description": "Hijacks Quilt in order to load NilLoader in non-JavaAgent-friendly environments. Apologies.",
			"contributors": {
				"LemmaEOF": "Owner",
				"unascribed": "NilLoader dev"
			},
			"contact": {
				"homepage": "https://lemmaeof.gay/",
				"issues": "https://github.com/LemmaEOF/NilBootstrap/issues",
				"sources": "https://github.com/LemmaEOF/NilBootstrap"
			},
			"icon": "assets/nilbootstrap/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"pre_launch": "gay.lemmaeof.nilbootstrap.NilBootstrap"
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": "*"
			},
			{
				"id": "nilloader",
				"versions": "*"
			},
			{
				"id": "jna",
				"versions": "*"
			}
		]
	}
}
